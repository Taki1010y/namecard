<%= form_for(@user, url: namecard_index_path, method: :post) do |f| %>
    <p>
        <%= f.label :name, "氏名" %><br>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :school, "学校名" %><br>
        <%= f.text_field :school %>
    </p>
    <p>
        <%= f.label :email, "メールアドレス" %><br>
        <%= f.email_field :email %>
    </p>

    <%= f.submit "登録する"%>
    
<% end %>